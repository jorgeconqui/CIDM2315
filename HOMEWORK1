using System;

namespace Homework1
{
    class Program
    {
        static void Main(string[] args)
        {
            double X = 2.5;
            double Y = 3.3;
            double Z = 4 * X * X + 3 * Y;
            
            Console.WriteLine($"The value of Z when X = {X} and Y = {Y} is: {Z}");
        }
    }
}
